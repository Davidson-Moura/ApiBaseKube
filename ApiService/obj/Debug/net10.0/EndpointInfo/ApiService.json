                "$ref": "#/components/schemas/UserCreateModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/UserCreateModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
